local function helth()
	local script = Instance.new('LocalScript', Health)

	local ply=game.Players.LocalPlayer.Character
	local h=ply:WaitForChild("Humanoid")
	local txt=script.Parent
	txt.Text=h.Health
	local function set()
		if h.Health<=30 then
			txt.TextColor3=Color3.fromRGB(255,0,0)
		else
			txt.TextColor3=Color3.fromRGB(0,0,0)
		end
		local he=h.Health*10
		he=math.round(he)
		he=he/10
		txt.Text=he
	end
	h.HealthChanged:Connect(set)
end
coroutine.wrap(helth)()
