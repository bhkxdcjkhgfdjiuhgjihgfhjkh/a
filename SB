local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Im Stupid Hub for slap battle", "Ocean")

  local a = Window:NewTab("script")
    local aSection = a:NewSection("slap battle")

  aSection:NewButton("r20", "r20", function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/cheesynob39/R2O/main/LOADSTRING.lua"))() 
        loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
    end)
  aSection:NewButton("0P", "0P", function()
        loadstring(game:HttpGet(("https://raw.githubusercontent.com/ionlyusegithubformcmods/1-Line-Scripts/main/Slap%20Battles")))()
    end)
  aSection:NewButton("slap r", "r20", function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/cheesynob39/R2O/main/LOADSTRING.lua"))()
        loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
    end)
local aa = Window:NewTab("q to toggle ui")
    local aaSection = aa:NewSection("q to toggle ui")
aaSection:NewKeybind("KeybindText", "KeybindInfo", Enum.KeyCode.Q, function()
	Library:ToggleUI()
end)
