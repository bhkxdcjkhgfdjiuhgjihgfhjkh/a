local m1reset = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local toggle = Instance.new("TextButton")
local AD = Instance.new("TextLabel")
local WS = Instance.new("TextLabel")
local Health = Instance.new("TextLabel")
local abed=false

m1reset.Name = "dash cancel pc"
m1reset.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
m1reset.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

Frame.Parent = m1reset
Frame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Frame.BorderColor3 = Color3.fromRGB(0, 0, 0)
Frame.BorderSizePixel = 0
Frame.Position = UDim2.new(-0.00097965484, 0, 0.577355981, 0)
Frame.Size = UDim2.new(0, 79, 0, 76)

toggle.Name = "toggle"
toggle.Parent = Frame
toggle.BackgroundColor3 = Color3.fromRGB(139, 139, 139)
toggle.BorderColor3 = Color3.fromRGB(0, 0, 0)
toggle.BorderSizePixel = 0
toggle.Position = UDim2.new(-0.0229055192, 0, 0.337382466, 0)
toggle.Size = UDim2.new(0, 80, 0, 50)
toggle.Font = Enum.Font.SourceSans
toggle.Text = "ENABLE: false"
toggle.TextColor3 = Color3.fromRGB(255, 0, 0)
toggle.TextSize = 14.000

AD.Name = "AD"
AD.Parent = m1reset
AD.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
AD.BorderColor3 = Color3.fromRGB(0, 0, 0)
AD.BorderSizePixel = 0
AD.Position = UDim2.new(0, 0, 1, -30)
AD.Size = UDim2.new(0, 150, 0, 30)
AD.Font = Enum.Font.SourceSans
AD.Text = "A/D dash:0"
AD.TextColor3 = Color3.fromRGB(0, 0, 0)
AD.TextSize = 14.000

WS.Name = "WS"
WS.Parent = m1reset
WS.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
WS.BorderColor3 = Color3.fromRGB(0, 0, 0)
WS.BorderSizePixel = 0
WS.Position = UDim2.new(0, 0, 1, -60)
WS.Size = UDim2.new(0, 150, 0, 30)
WS.Font = Enum.Font.SourceSans
WS.Text = "W/S dash:0"
WS.TextColor3 = Color3.fromRGB(0, 0, 0)
WS.TextSize = 14.000

Health.Parent = m1reset
Health.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Health.BorderColor3 = Color3.fromRGB(255, 255, 255)
Health.BorderSizePixel = 0
Health.BackgroundTransparency=1
Health.TextStrokeColor3=Color3.new(1,1,1)
Health.TextStrokeTransparency=0
Health.Position = UDim2.new(0.47, 0, 0.45, 0)
Health.Size = UDim2.new(0, 50, 0, 50)
Health.Font = Enum.Font.SourceSans
Health.Text = ""
Health.TextColor3 = Color3.fromRGB(0, 0, 0)
Health.TextSize = 25

local function drag_script()
	local script = Instance.new('LocalScript', Frame)

	local Drag = script.Parent.Parent.Frame
	gsTween = game:GetService("TweenService")
	local UserInputService = game:GetService("UserInputService")
	local dragging
	local dragInput
	local dragStart
	local startPos
	local function update(input)
		local delta = input.Position - dragStart
		local dragTime = 0.04
		local SmoothDrag = {}
		SmoothDrag.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		local dragSmoothFunction = gsTween:Create(Drag, TweenInfo.new(dragTime, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), SmoothDrag)
		dragSmoothFunction:Play()
	end
	Drag.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = Drag.Position
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	Drag.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)
	UserInputService.InputChanged:Connect(function(input)
		if input == dragInput and dragging and Drag.Size then
			update(input)
		end
	end)

end
coroutine.wrap(drag_script)()
local function RCLA_fake_script()
	local script = Instance.new('LocalScript', toggle)

	local o = 0 
	local fd = 0
	local dd
	local txt = script.Parent
	local player = game.Players.LocalPlayer 
	local character = player.Character
	local hrp = character.HumanoidRootPart
	local ply = game.Players.LocalPlayer.Character
	local chr = ply:GetAttribute("Character")
	local vim = game:GetService("VirtualInputManager")
	local Humanoid = character:WaitForChild("Humanoid")
	local UserInputService = game:GetService("UserInputService")

	local function decode(inputString)
		if inputString=="" or inputString==nil then return nil else
		local parts = {}
		for part in string.gmatch(inputString, "([^;]+)") do
			table.insert(parts, part)
		end
		return parts[1]
		end
	end
	script.Parent.MouseButton1Click:Connect(function()
		if o == 1 then
			o = 0
			txt.TextColor3 = Color3.fromRGB(255,0,0)
			txt.Text = "Enable: False"
		else
			o = 1
			txt.TextColor3	 = Color3.fromRGB(0,255,0)
			txt.Text = "Enable: True"
		end 
	end)
	local function fDash()
		local hrp = ply:FindFirstChild("HumanoidRootPart")
		local h = ply:FindFirstChild("Humanoid")
		if hrp and h then
			local target=character:GetAttribute("LastM1Hitted")
			abed=true
			fd=1
			local fp = Instance.new("Part",workspace)
			fp.Anchored=true
			fp.CanCollide = false
			fp.Transparency = 1
			local anim = Instance.new("Animation")
			anim.AnimationId = "rbxassetid://10480793962"

			local attach = Instance.new("Attachment",hrp)

			local LinearVelocity = Instance.new("LinearVelocity",attach)

			local part = Instance.new("Part",workspace)
			part.Anchored = true
			part.CanCollide = false
			part.Transparency = 0
			if chr=="Blade" then
				part.CFrame = hrp.CFrame * CFrame.new(35,0,0)
				fp.CFrame = hrp.CFrame * CFrame.new(35,0,0)
			else
				part.CFrame = hrp.CFrame * CFrame.new(30,0,0)
				fp.CFrame = hrp.CFrame * CFrame.new(30,0,0)
			end
			local animplay = h:LoadAnimation(anim)
			animplay:Play()

			LinearVelocity.MaxForce = 9999999
			LinearVelocity.VectorVelocity = (part.Position-hrp.Position).Unit*Vector3.new(150,0,150)
			LinearVelocity.Attachment0 = attach
			task.wait(0.15)
			attach:Destroy()
			part:Destroy()
			LinearVelocity:Destroy()
			fd=0
			abed=false
			wait(.05)
			ply.HumanoidRootPart.AssemblyLinearVelocity = Vector3.new(0,0,0)
			ply.HumanoidRootPart.CFrame=fp.CFrame*CFrame.Angles(0,math.rad(80),0)
			fp:Destroy()
			task.wait(0.05)
			vim:SendKeyEvent(true, Enum.KeyCode.Q, false, game)
			vim:SendKeyEvent(false, Enum.KeyCode.Q, false, game)
			local look = decode(target)
			if look ~=nil then
				local vp1 = workspace.Live:FindFirstChild(look)
				vp1=vp1.PrimaryPart
				for i = 1, 100 do
					hrp.CFrame = CFrame.lookAt(hrp.Position,Vector3.new(vp1.Position.X,hrp.Position.Y,vp1.Position.Z))
					task.wait()
				end
			end
		end
	end
	UserInputService.InputBegan:Connect(function(input, _gameProcessed)
		if input.KeyCode == Enum.KeyCode.E and not _gameProcessed then
			if o == 1 then
				o = 0
				txt.TextColor3 = Color3.fromRGB(255,0,0)
				txt.Text = "Enable: False"
			else
				o = 1
				txt.TextColor3	 = Color3.fromRGB(0,255,0)
				txt.Text = "Enable: True"
			end 
		end
	end)
	if chr=="Hunter" then
		dd=125
	elseif chr=="Bald" then
		dd=110
	else
		dd=125
	end

	local function onAnimationPlayed(animationTrack)
		if o == 1 and fd ~= 1 then
			if animationTrack.Animation.AnimationId == "rbxassetid://10480796021" then 
				if Humanoid.FloorMaterial~=Enum.Material.Air then Humanoid:ChangeState(Enum.HumanoidStateType.Jumping) end
				for _,animTrack in pairs(Humanoid:GetPlayingAnimationTracks())do 
					wait(.02)
					animTrack:Stop()
					local part = Instance.new("Part",workspace)
					part.Anchored = true
					part.CanCollide = false
					part.Transparency = 1
					part.CFrame = ply.HumanoidRootPart.CFrame * CFrame.new(-30,0,0)
					local hy=ply.HumanoidRootPart.AssemblyLinearVelocity.Y
					ply.HumanoidRootPart.AssemblyLinearVelocity = (part.Position-ply.HumanoidRootPart.Position).Unit*Vector3.new(dd,0,dd)
					ply.HumanoidRootPart.AssemblyLinearVelocity=Vector3.new(ply.HumanoidRootPart.AssemblyLinearVelocity.X, hy, ply.HumanoidRootPart.AssemblyLinearVelocity.Z)
					part:Destroy()
				end 
			end 
			if animationTrack.Animation.AnimationId == "rbxassetid://10480793962" then 
				if Humanoid.FloorMaterial~=Enum.Material.Air then Humanoid:ChangeState(Enum.HumanoidStateType.Jumping) end
				for _,animTrack in pairs(Humanoid:GetPlayingAnimationTracks())do 
					wait(.02)
					animTrack:Stop()
					local part = Instance.new("Part",workspace)
					part.Anchored = true
					part.CanCollide = false
					part.Transparency = 1
					part.CFrame = ply.HumanoidRootPart.CFrame * CFrame.new(30,0,0)
					local hy=ply.HumanoidRootPart.AssemblyLinearVelocity.Y
					ply.HumanoidRootPart.AssemblyLinearVelocity = (part.Position-ply.HumanoidRootPart.Position).Unit*Vector3.new(dd,0,dd)
					ply.HumanoidRootPart.AssemblyLinearVelocity=Vector3.new(ply.HumanoidRootPart.AssemblyLinearVelocity.X, hy, ply.HumanoidRootPart.AssemblyLinearVelocity.Z)
					part:Destroy()
				end 
			end
			if animationTrack.Animation.AnimationId == "rbxassetid://15290930205" then 
				local wall = Instance.new("Part",workspace)
				wall.Transparency=1
				wall.Anchored=true wall.Size=Vector3.new(10, 8, 2)
				wall.CFrame=hrp.CFrame*CFrame.new(0,0,10)
				task.wait(1) wall:Destroy()
			end
		end
	end 
	Humanoid.AnimationPlayed:Connect(onAnimationPlayed)
	UserInputService.InputBegan:Connect(function(input, _gameProcessed)
		if input.KeyCode == Enum.KeyCode.C and not _gameProcessed then
			if o == 1 then
				fDash()
			end 
		end
	end)

	game.StarterGui:SetCore("SendNotification", {Title = "Cooked by",Text = "Fuzruoycpu :)"})
end
coroutine.wrap(RCLA_fake_script)()
local function PNROJJ_fake_script()
	local script = Instance.new('LocalScript', AD)

	local col = 0
	local txt = 0
	local player = game.Players.LocalPlayer 
	local character = player.Character or player.CharacterAdded:Wait()
	local humanoid = character:WaitForChild("Humanoid")
	local function onAnimationPlayed(animationTrack)
		if animationTrack.Animation.AnimationId == "rbxassetid://10480796021" and abed==false then 
			col = 15
			script.Parent.Text = "A/D dash: "..col/10
		end
		if animationTrack.Animation.AnimationId == "rbxassetid://10480793962" and abed==false then 
			col = 15
			script.Parent.Text = "A/D dash: "..col/10
		end
	end
	humanoid.AnimationPlayed:Connect(onAnimationPlayed)
	while wait()do
		if col>0 then
			wait(.1)
			col=col-1
			math.round(col)
			txt=col/10
			script.Parent.Text = "A/D dash: "..txt
		end
	end
end
coroutine.wrap(PNROJJ_fake_script)()
local function KUVZ_fake_script()
	local script = Instance.new('LocalScript', WS)

	local col = 0
	local txt = 0
	local player = game.Players.LocalPlayer 
	local character = player.Character or player.CharacterAdded:Wait()
	local humanoid = character:WaitForChild("Humanoid")
	local function onAnimationPlayed(animationTrack)
		if animationTrack.Animation.AnimationId == "rbxassetid://10479335397" or animationTrack.Animation.AnimationId == "rbxassetid://13380255751" then 
			col =35
			script.Parent.Text = "W/S dash: "..col/10
		end
		if animationTrack.Animation.AnimationId == "rbxassetid://10491993682" then 
			col = 35
			script.Parent.Text = "W/S dash: "..col/10
		end
	end
	humanoid.AnimationPlayed:Connect(onAnimationPlayed)
	while wait()do
		if col>0 then
			wait(.1)
			col=col-1
			math.round(col)
			txt=col/10
			script.Parent.Text = "W/S dash: "..txt
		end
	end
end
coroutine.wrap(KUVZ_fake_script)()
